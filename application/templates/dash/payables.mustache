<input type="hidden" id="report-payables-vendor-defaultname" value="{{report_vendor_name}}">

<form action="" method="POST">
	<div class="form report-form">
		<div class="row column-10">
			<span class="oop-oop-oop inline-label text-right">Payables as of: </span>
			<span class="oop-oop"><input type="text" class="report-balance-date" name="date" value="{{report_date}}"></span>
			<span class="oop text-right inline-label text-bold">or</span>
			<span class="oop-oop inline-label text-right">Vendor</span>
			<span class="ehhh-sexy-lady">
				<input type="hidden" id="report-payables-vendor" name="vendor_id" value="{{report_vendor_id}}">
			</span>
			<span class="oop-oop-oop">&nbsp;</span>
			<span class="oop">&nbsp;</span>
			<span class="oop-oop-oop"><a href="#" class="button report-print">Print Report</a></span>
		</div>
		<div class="clear"></div>
	</div>
</form>

<div class="clear"></div>

<div id="report-content">
	{{>dash_print_payables}}
</div>