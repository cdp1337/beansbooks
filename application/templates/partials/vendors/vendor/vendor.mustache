<li class="vendor-vendor" rel="{{id}}">
	<span class="vendor-vendor-name text-left">{{display_name}}</span>
	<span class="vendor-vendor-phone text-center">{{phone_number}}{{^phone_number}}Not Available{{/phone_number}}</span>
	<span class="vendor-vendor-email text-center">{{email}}{{^email}}Not Available{{/email}}</span>
	<span class="vendor-vendor-balance text-right">{{balance_current_formatted}}</span>
	<span class="vendor-vendor-actions text-right">
		<a href="/vendors/vendor/{{id}}">View</a>
	</span>
</li>